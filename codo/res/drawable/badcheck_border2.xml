<?xml version="1.0" encoding="UTF-8"?><layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:width="100dp" android:height="60dp">
    <shape>
      <stroke android:width="4dp" android:color="#2534b9"/>
      <corners android:radius="8dp"/>
      <solid android:color="#ffffff"/>
    </shape>
  </item>
    <item

        android:left="28dp"
        android:top="30dp"
        android:width="35dp"
        android:height="5dp">
        <rotate
            android:fromDegrees="-55">
            <shape android:shape="rectangle">
                <solid android:color="#2534b9" />
                <corners android:radius="5dp"/>
            </shape>
        </rotate>
    </item>
    <item

        android:left="48dp"
        android:top="27dp"
        android:width="20dp"
        android:height="5dp">
        <rotate
            android:fromDegrees="65">
            <shape android:shape="rectangle">
                <solid android:color="#2534b9" />
                <corners android:radius="5dp"/>
            </shape>
        </rotate>
    </item>

</layer-list>
